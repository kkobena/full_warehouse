                                </configuration>
                                <goals>
                                    <goal>npm</goal>
                                </goals>
                                <id>webapp build prod</id>
                                <phase>generate-resources</phase>
                            </execution>
                        </executions>
                        <groupId>com.github.eirslett</groupId>
                    </plugin>
                </plugins>
            </build>
            <id>prod</id>
            <properties>
